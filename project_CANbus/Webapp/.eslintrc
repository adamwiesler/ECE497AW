{
"extends": "airbnb",
"rules": {
     "no-console": 0,
     "arrow-body-style": ["error", "always"],
     "indent": ["error", 2],
     "new-cap": ["error", { "properties": false }],
     "no-param-reassign": ["error", { "props": false }],
     "no-underscore-dangle": ["error", { "allow": ["_raw", "_raw.user_id"] }],
     "no-plusplus": 0,
},
"env": {
 "browser": true,
 "node": true,
 "es6": true,
 "jquery": true,
 "mocha": true,
},
"globals": {
  "Handlebars"
  },
}
